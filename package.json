{
	"name": "ngx-k",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "npm run build:lib",
		"build:lib": "run-s build:lib:core build:lib:components build:lib:components-bootstrap build:lib:auth build:lib:form-builder build:lib:form-builder-bootstrap",
		"build:lib:core": "ng build core --prod",
		"build:lib:components": "ng build components --prod",
		"build:lib:components-bootstrap": "ng build components-bootstrap --prod",
		"build:lib:auth": "ng build auth --prod",
		"build:lib:form-builder": "ng build form-builder --prod",
		"build:lib:form-builder-bootstrap": "ng build form-builder-bootstrap --prod",
		"publish": "npm run publish:lib",
		"publish:lib": "run-s publish:lib:core publish:lib:components publish:lib:components-bootstrap publish:lib:auth publish:lib:form-builder publish:lib:form-builder-bootstrap",
		"publish:lib:core": "npm publish dist/core --access public",
		"publish:lib:components": "npm publish dist/components --access public",
		"publish:lib:components-bootstrap": "npm publish dist/components-bootstrap --access public",
		"publish:lib:auth": "npm publish dist/auth --access public ",
		"publish:lib:form-builder": "npm publish dist/form-builder --access public",
		"publish:lib:form-builder-bootstrap": "npm publish dist/form-builder-bootstrap --access public",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^11.0.1",
