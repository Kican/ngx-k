		"bootstrap": "^4.5.3",
		"ngx-toastr": "^13.1.0",
		"rxjs": "~6.6.3",
		"tslib": "^2.0.3",
		"zone.js": "~0.11.3",
		"@mdi/font": "^5.8.55",
