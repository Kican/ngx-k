{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@ngx-k/core": {
      "projectType": "library",
      "root": "projects/ngx-k/core",
      "sourceRoot": "projects/ngx-k/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-k/core/tsconfig.lib.json",
            "project": "projects/ngx-k/core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-k/core/src/test.ts",
            "tsConfig": "projects/ngx-k/core/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-k/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-k/core/tsconfig.lib.json",
              "projects/ngx-k/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-k/form-builder": {
      "projectType": "library",
      "root": "projects/ngx-k/form-builder",
      "sourceRoot": "projects/ngx-k/form-builder/src",
