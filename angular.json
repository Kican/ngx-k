          "options": {
            "main": "projects/ngx-k/form-builder/src/test.ts",
            "tsConfig": "projects/ngx-k/form-builder/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-k/form-builder/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-k/form-builder/tsconfig.lib.json",
              "projects/ngx-k/form-builder/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
